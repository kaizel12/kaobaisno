    "/api/kuburancanvas": {
      "get": {
        "summary": "Generate Kuburan Canvas Image",
        "description": "Generates a styled image with background and border effects",
        "tags": ["Image Generator"],
        "parameters": [{
          "name": "url",
          "in": "query",
          "description": "URL of the image to be processed",
          "required": true,
          "schema": {
            "type": "string",
            "example": "https://example.com/image.jpg"
          }
        }],
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "image": {
                      "type": "string",
                      "description": "Base64 encoded generated image"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Missing or invalid URL parameter",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error generating image",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
