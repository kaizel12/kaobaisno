  "paths": {
    "/api/kuburancanvas": {
      "get": {
        "summary": "Generate Kuburan Canvas Image",
        "description": "Generates a styled image with background and border effects",
        "tags": ["Image Generator"],
        "parameters": [{
          "name": "url",
          "in": "query",
          "description": "URL of the image to be processed",
          "required": true,
          "schema": {
            "type": "string",
            "example": "https://example.com/image.jpg"
          }
        }],
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "image": {
                      "type": "string",
                      "description": "Base64 encoded generated image"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Missing or invalid URL parameter",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error generating image",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
        "/api/welcomecard": {
      "get": {
        "summary": "Generate Welcome Card",
        "description": "Menghasilkan gambar kartu ucapan berdasarkan parameter yang diberikan.",
        "parameters": [
          {
            "name": "linkbekron",
            "in": "query",
            "required": true,
            "description": "URL gambar latar belakang.",
            "schema": {
              "type": "string",
              "example": "https://example.com/background.jpg"
            }
          },
          {
            "name": "linkavatar",
            "in": "query",
            "required": true,
            "description": "URL gambar avatar.",
            "schema": {
              "type": "string",
              "example": "https://example.com/avatar.jpg"
            }
          },
          {
            "name": "judul",
            "in": "query",
            "required": true,
            "description": "Judul yang akan ditampilkan pada kartu.",
            "schema": {
              "type": "string",
              "example": "Selamat Datang"
            }
          },
          {
            "name": "desk",
            "in": "query",
            "required": true,
            "description": "Deskripsi yang akan ditampilkan pada kartu.",
            "schema": {
              "type": "string",
              "example": "Kami senang Anda bergabung dengan kami."
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Gambar kartu ucapan berhasil dibuat.",
            "content": {
              "image/png": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "400": {
            "description": "Parameter yang diperlukan tidak lengkap.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": { "type": "string" },
                    "required": {
                      "type": "array",
                      "items": { "type": "string" }
                    }
                  },
                  "example": {
                    "error": "Missing parameters",
                    "required": ["linkbekron", "linkavatar", "judul", "desk"]
                  }
                }
              }
            }
          },
          "405": {
            "description": "Metode HTTP tidak diperbolehkan.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": { "type": "string" }
                  },
                  "example": {
                    "error": "Method not allowed"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Terjadi kesalahan saat membuat gambar.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": { "type": "string" },
                    "details": { "type": "string" }
                  },
                  "example": {
                    "error": "Error generating image",
                    "details": "Detail error"
                  }
                }
              }
            }
          }
        }
      }
    }, 
